source "https://rubygems.org"

gem "jekyll"

# Plugins
group :jekyll_plugins do
  gem 'jekyll-sitemap'
  gem 'jekyll-feed'
  gem "jekyll-gist"
  gem "jekyll-paginate"
  gem "jekyll-seo-tag"
  gem "jemoji"
  gem "webrick"
  gem "jekyll-github-metadata"
  gem 'wdm', '~> 0.1.1', :install_if => Gem.win_platform?
end
